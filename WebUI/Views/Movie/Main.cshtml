@model WebUI.Models.MoviesListViewModel

@{
    ViewBag.Title = "Main";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@foreach (var p in Model.Movies)
{
    Html.RenderPartial("MovieSummary", p);

}

<div class="pager">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("Main", new { page = x, category = Model.CurrentYearCategory }))
</div>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/star-rating")



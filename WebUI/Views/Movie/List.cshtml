@model WebUI.Models.MoviesListViewModel

@{
    ViewBag.Title = "Movies";
}


@*<div class="page-title col-md-12">
    <p class="page-title-text">master movie</p>
</div>*@

<div id="categories">
    @{ Html.RenderAction("Menu", "Nav");}
</div>
<div id="content" class="container body-content">
    @foreach (var p in Model.Movies)
    {
        Html.RenderPartial("MovieSummary", p);
    }


</div>
<div class="pager">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new { page = x, category = Model.CurrentYearCategory, searchString = ViewBag.SearchString }))
</div>  

<input type="button" id="rate" value="RATE" />   @*this should be removed*@ 

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/star-rating")
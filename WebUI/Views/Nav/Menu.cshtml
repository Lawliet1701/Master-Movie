@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Movie")

@foreach (var link in Model)
{
    @Html.RouteLink(link + "s", new
    {
        controller = "Movie",
        action = "List",
        category = link,
        page = 1
    },
    new {
        @class = link == ViewBag.SelectedCategory ? "selected" : null
    })
}
